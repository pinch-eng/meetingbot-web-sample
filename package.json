{
  "name": "meetingbot-web-sample",
  "version": "1.0.0",
  "description": "Zoom Meeting Bot Web Sample with TypeScript and React",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "build": "cd client && npm run build && cd ../server && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "axios": "^1.7.9",
    "body-parser": "^1.20.2",
    "btoa": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "jsrsasign": "^11.1.0",
    "node-cron": "^3.0.2",
    "nodemon": "^3.1.7",
    "puppeteer": "^23.5.2",
    "websocket": "^1.0.34",
    "@zoomus/websdk": "^2.18.2",
    "concurrently": "^8.2.2"
  }
}
