# 服务器配置
PORT=30015
NODE_ENV=development

# Zoom SDK 凭证
# 从 Zoom 开发者门户获取: https://marketplace.zoom.us/
ZOOM_SDK_KEY=your_zoom_sdk_key
ZOOM_SDK_SECRET=your_zoom_sdk_secret

# React 应用配置
# 注意: React 环境变量必须以 REACT_APP_ 开头
REACT_APP_ZOOM_SDK_KEY=your_zoom_sdk_key
REACT_APP_API_URL=http://localhost:30015/api

# Zoom Webhook Secret Token:
ZOOM_WEBHOOK_SECRET_TOKEN_EX=ZOOM_WEBHOOK_SECRET_TOKEN_HERE

# Zoom Server-to-Server Credentials API Credentials:
ACCOUNT_ID=ZOOM_SDK_ACCOUNT_ID
CLIENT_ID=ZOOM_SDK_CLIENT_ID
CLIENT_SECRET=ZOOM_SDK_CLIENT_SECRET

# Zoom Meeting SDK Credentials: 
ZOOM_MSDK_KEY=ZOOM_MSDK_KEY_HERE
ZOOM_MSDK_SECRET=ZOOM_MSDK_SECRET_HERE